<tr>
        <td><%= link_to instructor.id, instructor_path(instructor) %></td>
        <td><%= instructor.nombre %></td>
        <td><%= instructor.apellido %></td>
        <td><%= instructor.cedula %></td>
        <td><%= instructor.email %></td>
        <td><%= instructor.titulo %></td>
        <td><%= instructor.centro.nombre %></td>

        <td><%=l instructor.created_at %></td>
        <td>
          <%= link_to t('.editar', :default => t("helpers.links.Editar")),
                      edit_instructor_path(instructor), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.Eliminar")),
                      instructor_path(instructor),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirmar", :default => 'Estas Seguro?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>