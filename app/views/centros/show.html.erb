<%- model_class = Centro -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @centro.nombre %></dd>
 <dt><strong><%= model_class.human_attribute_name(:ciudad)%>:</strong></dt>
 <dd><%= centro.ciudad %></dd>
  <dt><strong><%= model_class.human_attribute_name(:codigo) %>:</strong></dt>
  <dd><%= centro.codigo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:regional_id) %>:</strong></dt>
  <dd><%= @centro.regional_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.Atras")),
              regional_centros_path(@regional), :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.Editar")),
              edit_regional_centro_path(@regional,@centro), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.Eliminar")),
              regional_centro_path(@regional,@centro),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirmar", :default => 'Estas seguro?')) },
              :class => 'btn btn-danger' %>
</div>
